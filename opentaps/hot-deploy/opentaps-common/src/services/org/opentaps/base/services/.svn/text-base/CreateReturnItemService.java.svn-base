            GenericValue userGv = (GenericValue) mapValue.get("userLogin");
            if (userGv != null) {
                service.setUser(new User(userGv, userGv.getDelegator()));
            }
        }
        return service;
    }

    /**
     * Construct a <code>CreateReturnItemService</code> from the given output <code>Map</code>.
     * @param mapValue the service output <code>Map</code>
     */
    public static CreateReturnItemService fromOutput(Map<String, Object> mapValue) {
        CreateReturnItemService service = new CreateReturnItemService();
        service.putAllOutput(mapValue);
        return service;
    }
